<div id="login">
<h1>Log in</h1>
<br>
<%= form_tag("/login") do %>
  <%= label_tag 'username', "Username" %>
  <%= text_field_tag :username %><br>
  <%= label_tag 'password', "Password" %>
  <%= password_field_tag :password %><br><br>
  <%= submit_tag "Log in", class: "btn btn-primary btn-xs" %>
<% end %>
</div>
